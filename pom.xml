                    <tagBase>https://barricada.hi.inet/repositorio/boi/europ-iaas/app-conf/sdc-server/tags</tagBase>
                </configuration>
            </plugin>
            <!-- Plugin HUDSON -->
            <plugin>
                <groupId>org.jvnet.hudson.tools</groupId>
